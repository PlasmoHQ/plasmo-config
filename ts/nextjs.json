{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "es2015",
    "declaration": false,
    "declarationMap": false,
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "module": "esnext",
    "jsx": "preserve"
  },
  "exclude": ["node_modules"]
}
